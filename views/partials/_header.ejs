<header>
  <nav class="navbar navbar-expand-md navbar-dark bg-success">
    <a class="navbar-brand" href="/urls">TinyApp</a>
    <button
      class="navbar-toggler"
      type="button"
      data-toggle="collapse"
      data-target="#navbarNavAltMarkup"
      aria-controls="navbarNavAltMarkup"
      aria-expanded="false"
      aria-label="Toggle navigation"
    >
      <span class="navbar-toggler-icon"></span>
    </button>
    <div class="collapse navbar-collapse" id="navbarNavAltMarkup">
      <div class="navbar-nav">
        <a class="nav-item nav-link" href="/urls">My URLs</a>
        <a class="nav-item nav-link" href="/urls/new">Create New URL</a>
      </div>
    </div>
    <div>
      <% if (!user) {%>
        <a href="/login">
          <button type="button" class="btn btn-outline-light my-2 my-sm-0">Login</button>
        </a>
        <a href="/register">
          <button type="button" class="btn btn-outline-light my-2 my-sm-0">Register</button>
        </a>
      <% } %>
      <% if (user) {%>
        <form class="form-inline my-2 my-lg-0" method="POST" action="/logout">
          Logged in as: <%= user.email %>!
          <div class="col 1"></div>
          <button type="submit" class="btn btn-outline-light my-2 my-sm-0">Logout</button>
        </form>
      <% } %>
    </div>
  </nav>
</header>